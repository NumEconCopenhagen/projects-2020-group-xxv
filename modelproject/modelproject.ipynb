    "k = sm.symbols('k')\n",
    "alpha = sm.symbols('alpha',positive =True)\n",
    "delta = sm.symbols('delta',positive =True)\n",
    "sigma = sm.symbols('sigma',positive =True)\n",
    "s = sm.symbols('s',positive =True)\n",
    "g = sm.symbols('g')\n",
    "n = sm.symbols('n')\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 195,
   "metadata": {},
   "outputs": [],
   "source": [
    "objective = (1/((1+g)*(1+n)))*(s*((alpha*k**((sigma-1)/sigma))+(1-alpha))**((sigma/(sigma-1)))+(1-delta)*k)"
   ]
  },
